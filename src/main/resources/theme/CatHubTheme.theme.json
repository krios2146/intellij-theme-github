{
  "author": "Vladimir Kidyaev",
  "name": "CatHub Theme",
  "dark": true,
  "editorScheme": "/theme/CatHubTheme.xml",
  "colors": {
    "base00": "#0d1117",
    "base10": "#161b22",
    "base20": "#21262d",
    "base30": "#30363d",
    "base40": "#6e7681",
    "base50": "#8b949e",
    "base60": "#c9d1d9",
    "base100": "#ffffff",
    "faint": "#484f58",
    "muted": "#6e7681",
    "border": "#30363d",
    "borderMuted": "#21262d",
    "hover": "#21262d",
    "greenBtn": "#238636",
    "greenBtnHover": "#2ea043",
    "greenBtnBorder": "#2ea043",
    "greenBtnBorderHover": "#3fb950"
  },
  "ui": {
    "*": {
      "background": "base00",
      "foreground": "base60",
      "rowHeight": "24",
      "borderColor": "borderMuted",
      "selectionBackground": "base30",
      "selectionForeground": "base100",
      "infoForeground": "muted",
      "disabledText": "faint",
      "separatorColor": "base20"
    },
    "Borders.color": "borderMuted",
    "Borders.ContrastBorderColor": "border",
    "Tree.border": "4,12,4,12",
    "EditorTabs": {
      "tabInsets": "-3,12,-4,8"
    },
    "ActionButton" : {
      "hoverBorderColor": "hover",
      "hoverBackground": "hover",
      "pressedBackground": "base30",
      "pressedBorderColor": "base30"
    },
    "MainToolbar.Icon.hoverBackground": "hover",
    "MainToolbar.Icon.pressedBackground": "base30",
    "MainToolbar.Dropdown.hoverBackground": "hover",
    "PackageSearch.PackageTag.hoverBackground": "hover",
    "PackageSearch.SearchResult.hoverBackground": "hover",
    "PackageSearch.SearchResult.PackageTag.hoverBackground": "hover",
    "StatusBar.hoverBackground": "hover",
    "StatusBar.Widget.hoverBackground": "hover",
    "ToolWindow.Header.inactiveBackground": "base00",
    "ToolWindow.HeaderTab.selectedInactiveBackground": "base30",
    "RunWidget": {
      "background": "greenBtn",
      "foreground": "base100",
      "hoverBackground": "greenBtnHover",
      "runningBackground": "greenBtnHover",
      "pressedBackground": "#196c2e"
    },
    "Button": {
      "foreground": "base60",
      "startBackground": "base20",
      "endBackground": "base20",
      "startBorderColor": "border",
      "endBorderColor": "border",
      "disabledBorderColor": "borderMuted",
      "shadowWidth": 0,
      "default": {
        "foreground": "base100",
        "startBackground": "greenBtn",
        "endBackground": "greenBtn",
        "startBorderColor": "greenBtnBorder",
        "endBorderColor": "greenBtnBorder"
      }
    },
    "MenuItem.acceleratorForeground": "muted",
    "Label.disabledForeground": "faint",
    "Tree.selectionInactiveBackground": "base20",
    "Tree.selectionInactiveForeground": "base60",
    "Tree.foreground": "base60",
    "NotificationsToolwindow.newNotification.background": "base20",
    "NotificationsToolwindow.newNotification.hoverBackground": "base30",
    "Component.disabledBorderColor": "borderMuted",
    "TabbedPane.contentAreaColor": "borderMuted",
    "MenuItem.acceleratorSelectionForeground": "base50",
    "MenuItem.disabledForeground": "faint",
    "Plugins.lightSelectionBackground": "hover",
    "Popup.separatorForeground": "base50"
  }
}
